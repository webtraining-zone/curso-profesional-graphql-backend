type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  createdAt: DateTime!
  modifiedAt: DateTime!
}

type Book {
    id: ID! @unique
    title: String!
    description: String
    image: String
    thumbnail: String
    createdAt: DateTime!
    updatedAt: DateTime!
}
